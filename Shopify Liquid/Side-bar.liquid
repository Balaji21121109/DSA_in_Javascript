<div class="main_side_bar">
  
  <button onclick="toggleSidebar()" class="side_bar">
  <svg class="svg_icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="22" height="22" viewBox="0 0 22 22" fill="none">
<rect width="22" height="22" fill="url(#pattern0)"/>
<defs>
<pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
<use xlink:href="#image0_1904_1012" transform="scale(0.00195312)"/>
</pattern>
<image id="image0_1904_1012" width="512" height="512" xlink:href="data:image/png;base64,"/>
</defs>
</svg>
  {{ section.settings.text_menu }}</button>
  <div class="main_sidebar_container" id="sidebarContainer" style="display: none;">
    {% for block in section.blocks %}
      <div class="main_sidebar_wrapper">
        <a href={{ block.settings.collection_link }}>
          <div class="collections">{{ block.settings.collection_text }}</div>
        </a>
      </div>
    {% endfor %}
  </div>
</div>

<script>
  function toggleSidebar() {
    var sidebarContainer = document.getElementById("sidebarContainer");
    if (sidebarContainer.style.display === "none") {
      sidebarContainer.style.display = "block";
    } else {
      sidebarContainer.style.display = "none";
    }
  }
</script>
{% style %}
  .svg_icon {
  display: block;
  width: 20px; /* Adjust the width and height as needed */
  height: 20px;
  margin: 0 auto; /* Center the SVG horizontally */
  margin-top: 5px; /* Adjust the margin as needed */
}

  .main_sidebar_container{
       position: fixed;
    top: 30%;
    z-index: 9999;
    right:45px;
  }
  .side_bar{
        position: fixed;
    top: 30%;
    z-index: 9999;
    right:0;
    background:#5BB7A1;
    border: none;
     display: flex;
    flex-direction: column;
    color: white;
  }
  .main_sidebar_wrapper a{
       text-decoration: none;
  }
  .main_sidebar_wrapper:nth-of-type(odd) {
background: #AFE7DE;
}
  .collections{
    color: white;
    text-align: center;
    font-size: 12px;
font-family: Montserrat;
font-style: normal;
font-weight: 400;
line-height: 20px;
letter-spacing: 2px;
padding: 10px;
  }
    
.main_sidebar_wrapper:nth-of-type(even) {
  background: #7BD3C5;
}
  .main_sidebar_container{
    width: 22%;
  }
  @media only screen and (max-width:749px){
    .main_sidebar_container{
  width: 50%;
      border: 1px solid #7BD3C5;
}
  }

{% endstyle %}
{% schema %}
  {
    "name": "side-bar",
    "settings":[
      {
        "type":"html",
        "id":"svg_menu",
        "label":"SVG"
      },
      {
        "type":"text",
        "id":"text_menu",
        "label":"Text",
        "default":"Menu"
      }
    ],
    "blocks": [
      {
        "name":"block",
        "type":"block",
        "settings":[
          
          {
            "type":"text",
            "id":"collection_text",
            "label":"Text"
          },
          {
          	"type": "url",
            "id": "collection_link",
              "label": "Collection link"
           }
        ]
      }
    ],
    "presets":[
      {
        "name":"side-bar"
      }
    ]
  }
{% endschema %}
