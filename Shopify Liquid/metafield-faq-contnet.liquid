{{ 'static-page-accordion.css' | asset_url | stylesheet_tag }}

<div class="static-pages page-width section-{{ section.id }}-padding">
  <div class="static__page-title">
    <h2>{{ section.settings.page_title }}</h2>
  </div>
  
  <div class="static__collapsible-section">      
    <div class="collapsible-tab-title">
      
    </div>

    <div class="collapsible-wrapper">
      <div class="collapsible__link-tabs">
        <ul class="quick__link">
          {% for block in section.blocks %}
            {% if block.type == 'collapsible-tabs' %}
              {% if block.settings.quick_link_text != blank %}
                <li class="quick__link-tab" data-link-item onClick="collapsibleToggle('{{ block.settings.quick_link_id }}')">{{ block.settings.quick_link_text }}</li>
              {% endif %}
            {% endif %}
          {% endfor %}
        </ul>
      </div>
      
      <div class="accordian--tab">
        {% for block in section.blocks %}
          {% if block.type == 'collapsible-tabs' %}

            <div class="collapsible__link-tabs-sm">
              <details class="quick__link-tab-sm">
                <summary class="faq-detials-sm faq__summary-title-sm">
                  <div class="quick__link-title-sm">{{ block.settings.quick_link_text }}</div>
                  
                  <div class="collapsible__faq-icon">
                    <svg width="20" height="14" viewBox="0 0 20 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path d="M2.35 0L10 8.65317L17.65 0L20 2.66397L10 14L0 2.66397L2.35 0Z" fill="#CBBB75"/>
                    </svg>
                  </div>
                </summary>
                
                <div class="collapsible__content-wrapper">
                  {% if block.settings.collapsible_heading_1 != blank %}
                    <details class="collapsible__faq-details">
                      <summary class="faq-detials flex faq__summary-title">
                        <div class="faq__accordion-title">
                          <span>{{ block.settings.collapsible_heading_1 }}</span>
                        </div>
                        
                        <div>
                          <svg width="20" height="14" viewBox="0 0 20 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M2.35 0L10 8.65317L17.65 0L20 2.66397L10 14L0 2.66397L2.35 0Z" fill="#CBBB75"/>
                          </svg>
                        </div>
                      </summary>
                      <div class="collapsible__faq-content">
                        {{ block.settings.collapsible_content_1 }}
                      </div>
                    </details>
                  {% endif %}
      
                  {% if block.settings.collapsible_heading_2 != blank %}
