{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

{% schema %}
  {
  "name": "Static Pages Accordion",
  "settings": [
    {
      "type": "text",
      "id": "page_title",
      "label": "Page Title"
    },
    {
      "type": "header",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 200,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 200,
      "step": 4,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 36
    }
  ],
  "blocks": [
    {
      "type": "collapsible-tabs",
      "name": "Collapsible Tabs",
      "settings": [
        {
          "type": "text",
          "label": "Quick Link Text",
          "id": "quick_link_text"
        },
        {
          "type": "text",
          "label": "Quicks link ID",
          "id": "quick_link_id"
        },
        {
          "type": "header",
          "content": "Collapisble Row-1"
        },
        {
          "type": "text",
          "id": "collapsible_heading_1",
          "label": "Collapsible Heading 1"
        },
        {
          "type": "richtext",
          "id": "collapsible_content_1",
          "label": "Collapsible Content 1"
        },
